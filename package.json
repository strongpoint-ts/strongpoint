{
  "name": "@typepoint/core",
  "version": "0.10.2",
  "description": "Library for defining, serving and consuming strongly typed endpoints in TypeScript",
  "main": "index.js",
  "private": true,
  "pre-commit": {
    "run": "check"
  },
  "scripts": {
    "check": "npm run lint && npm run build && npm run test",
    "clean": "npm run clean:dist",
    "clean:dist": "rimraf ./dist",
    "prebuild": "npm run clean:dist",
    "build": "npm run build:entry && npm run build:server && npm run build:client",
    "build:client": "tsc -p ./src/tsconfig.client.json",
    "build:entry": "tsc -p ./src/tsconfig.entry.json",
    "build:server": "tsc -p ./src/tsconfig.server.json",
    "lint": "tslint -p ./",
    "prepack": "npm run check",
    "pack": "ts-node ./pack.ts",
    "preship": "npm run pack",
    "ship": "cd ./dist && npm publish --access public",
    "test": "jest --config jest.config.js"
  },
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typepoint/typepoint.git"
  },
  "keywords": [
    "typepoint",
    "type-point",
    "typedpoint",
    "typed-point",
    "strongpoint",
    "strong-point",
    "strongly",
    "typed",
    "endpoints",
    "express",
    "TypeScript",
    "typesafe",
    "type-safe",
    "type",
    "safe",
    "rest",
    "api"
  ],
  "author": "Ben <codeandcats@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "contributors": [
    "Ben <codeandcats@gmail.com>",
    "Laurence Dougal Myers (https://github.com/laurence-myers)"
  ],
  "bugs": {
    "url": "https://github.com/typepoint/typepoint/issues"
  },
  "homepage": "https://github.com/typepoint/typepoint#readme",
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.test.js"
    ],
    "include": [
      "tests-output/src"
    ],
    "produce-source-map": true,
    "recursive": true,
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "devDependencies": {
    "@types/clone": "0.1.30",
    "@types/deep-freeze": "0.1.1",
    "@types/es6-promise": "3.3.0",
    "@types/express": "4.11.1",
    "@types/fs-extra": "5.0.1",
    "@types/get-port": "3.2.0",
    "@types/jest": "24.0.15",
    "@types/linq": "2.2.33",
    "@types/uuid": "3.4.3",
    "chalk": "2.3.2",
    "concurrently": "3.5.1",
    "express": "4.16.2",
    "fs-extra": "5.0.0",
    "get-port": "3.2.0",
    "inversify": "5.0.1",
    "jest": "24.8.0",
    "koa-router": "7.4.0",
    "linq": "3.0.9",
    "nyc": "11.4.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.3",
    "ts-jest": "24.0.2",
    "ts-node": "4.1.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2",
    "uuid": "3.2.1"
  },
  "dependencies": {
    "@types/joi": "10.4.2",
    "axios": "0.18.0",
    "clone": "2.1.1",
    "http-status-codes": "1.3.0",
    "joi": "10.6.0",
    "reflect-metadata": "0.1.12",
    "tsdv-joi": "0.0.13",
    "typed-tuple": "1.0.1"
  }
}
