    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typepoint/typepoint.git"
  },
  "bugs": {
    "url": "https://github.com/typepoint/typepoint/issues"
  },
  "homepage": "https://github.com/typepoint/typepoint#readme",
  "workspaces": {
    "packages": [
      "packages/client",
      "packages/e2e-tests",
      "packages/examples/todos-hooks-nextjs",
      "packages/examples/todos-hooks-parcel",
      "packages/express",
      "packages/fixtures",
      "packages/joiful",
      "packages/react",
      "packages/server",
      "packages/shared"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./packages/client/dist ./packages/express/dist ./packages/fixtures/dist ./packages/joiful/dist ./packages/server/dist ./packages/shared/dist",
    "lint": "eslint ./packages --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "prebuild": "yarn run clean",
    "validate-imports": "ts-node ./support/validateImports.ts",
    "validate": "yarn validate-imports && yarn workspace @typepoint/client validate && yarn workspace @typepoint/express validate && yarn workspace @typepoint/joiful validate && yarn workspace @typepoint/react validate && yarn workspace @typepoint/server && yarn workspace @typepoint/shared validate"
  },
  "devDependencies": {
